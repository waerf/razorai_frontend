n {\n      pendingRobots: [],\n    };\n  },\n  async created() {\n    await this.fetchPendingRobots();\n  },\n  methods: {\n    async fetchPendingRobots() {\n      try {\n        const response = await getPendingRobots();\n        this.pendingRobots = response.data;\n      } catch (error) {\n        console.error('获取待审核机器人失败:', error);\n      }\n    },\n    async approveRobot(id) {\n      try {\n        await approveRobot(id);\n        this.$message.success('审核通过成功');\n        await this.fetchPendingRobots();\n      } catch (error) {\n        console.error('审核通过失败:', error);\n        this.$message.error('审核通过失败');\n      }\n    },\n    async rejectRobot(id) {\n      try {\n        await rejectRobot(id);\n        this.$message.success('审核拒绝成功');\n        await this.fetchPendingRobots();\n      } catch (error) {\n        console.error('审核拒绝失败:', error);\n        this.$message.error('审核拒绝失败');\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.admin-review {\n  padding: 20px;\n}\n.review-list {\n  margin-top: 20px;\n}\n.review-item {\n  padding: 15px;\n  margin-bottom: 15px;\n  border: 1px solid #eee;\n  border-radius: 4px;\n}\n.actions {\n  margin-top: 10px;\n}\nbutton {\n  margin-right: 10px;\n  padding: 5px 10px;\n}\n</style>"],"mappings":"AAiBA,SAAAA,gBAAA,EAAAC,YAAA,EAAAC,WAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,aAAA;IACA;EACA;EACA,MAAAC,QAAA;IACA,WAAAC,kBAAA;EACA;EACAC,OAAA;IACA,MAAAD,mBAAA;MACA;QACA,MAAAE,QAAA,SAAAT,gBAAA;QACA,KAAAK,aAAA,GAAAI,QAAA,CAAAL,IAAA;MACA,SAAAM,KAAA;QACAC,OAAA,CAAAD,KAAA,gBAAAA,KAAA;MACA;IACA;IACA,MAAAT,aAAAW,EAAA;MACA;QACA,MAAAX,YAAA,CAAAW,EAAA;QACA,KAAAC,QAAA,CAAAC,OAAA;QACA,WAAAP,kBAAA;MACA,SAAAG,KAAA;QACAC,OAAA,CAAAD,KAAA,YAAAA,KAAA;QACA,KAAAG,QAAA,CAAAH,KAAA;MACA;IACA;IACA,MAAAR,YAAAU,EAAA;MACA;QACA,MAAAV,WAAA,CAAAU,EAAA;QACA,KAAAC,QAAA,CAAAC,OAAA;QACA,WAAAP,kBAAA;MACA,SAAAG,KAAA;QACAC,OAAA,CAAAD,KAAA,YAAAA,KAAA;QACA,KAAAG,QAAA,CAAAH,KAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDe