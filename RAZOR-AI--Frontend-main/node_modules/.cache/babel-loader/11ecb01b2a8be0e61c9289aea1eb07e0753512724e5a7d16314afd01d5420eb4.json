 name: 'AdminFeedbackPage',\n  data() {\n    return {\n      feedbacks: [],\n    };\n  },\n  async created() {\n    await this.fetchFeedbacks();\n  },\n  methods: {\n    async fetchFeedbacks() {\n      try {\n        const response = await getFeedbacks();\n        this.feedbacks = response.data;\n      } catch (error) {\n        console.error('获取用户反馈失败:', error);\n      }\n    },\n    async markAsRead(id) {\n      try {\n        await markFeedbackAsRead(id);\n        this.$message.success('标记为已读成功');\n        await this.fetchFeedbacks();\n      } catch (error) {\n        console.error('标记为已读失败:', error);\n        this.$message.error('标记为已读失败');\n      }\n    },\n    async deleteFeedback(id) {\n      try {\n        await deleteFeedback(id);\n        this.$message.success('删除反馈成功');\n        await this.fetchFeedbacks();\n      } catch (error) {\n        console.error('删除反馈失败:', error);\n        this.$message.error('删除反馈失败');\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.admin-feedback {\n  padding: 20px;\n}\n.feedback-list {\n  margin-top: 20px;\n}\n.feedback-item {\n  padding: 15px;\n  margin-bottom: 15px;\n  border: 1px solid #eee;\n  border-radius: 4px;\n}\n.actions {\n  margin-top: 10px;\n}\nbutton {\n  margin-right: 10px;\n  padding: 5px 10px;\n}\n</style>"],"mappings":"AAiBA,SAAAA,YAAA,EAAAC,kBAAA,EAAAC,cAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;IACA;EACA;EACA,MAAAC,QAAA;IACA,WAAAC,cAAA;EACA;EACAC,OAAA;IACA,MAAAD,eAAA;MACA;QACA,MAAAE,QAAA,SAAAT,YAAA;QACA,KAAAK,SAAA,GAAAI,QAAA,CAAAL,IAAA;MACA,SAAAM,KAAA;QACAC,OAAA,CAAAD,KAAA,cAAAA,KAAA;MACA;IACA;IACA,MAAAE,WAAAC,EAAA;MACA;QACA,MAAAZ,kBAAA,CAAAY,EAAA;QACA,KAAAC,QAAA,CAAAC,OAAA;QACA,WAAAR,cAAA;MACA,SAAAG,KAAA;QACAC,OAAA,CAAAD,KAAA,aAAAA,KAAA;QACA,KAAAI,QAAA,CAAAJ,KAAA;MACA;IACA;IACA,MAAAR,eAAAW,EAAA;MACA;QACA,MAAAX,cAAA,CAAAW,EAAA;QACA,KAAAC,QAAA,CAAAC,OAAA;QACA,WAAAR,cAAA;MACA,SAAAG,KAAA;QACAC,OAAA,CAAAD,KAAA,YAAAA,KAAA;QACA,KAAAI,QAAA,CAAAJ,KAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{}