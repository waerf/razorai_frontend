ansition: background-color 0.3s;\n        \n        &:hover {\n          background: #0E42D2;\n        }\n      }\n    }\n    \n    .stats-grid {\n      display: grid;\n      grid-template-columns: repeat(3, 1fr);\n      gap: 1.5rem;\n      padding: 1.5rem;\n    }\n    \n    .content-grid {\n      display: grid;\n      grid-template-columns: 1fr;\n      gap: 1.5rem;\n      padding: 1.5rem;\n      \n      @media (min-width: 1024px) {\n        grid-template-columns: repeat(2, 1fr);\n      }\n    }\n  }\n}\n</style>"],"mappings":";AAsDA;EACAA,OAAA;IACAC,WAAAC,KAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;QAAAC,IAAA,EAAAH;MAAA;IACA;IACAI,OAAA;MACA;IAAA;EAEA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}